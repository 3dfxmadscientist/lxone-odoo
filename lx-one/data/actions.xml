<?xml version="1.0"?>
<openerp>
	<data>
	
		<act_window name="All Files" 
			res_model="lx.update.file" 
			id="lx-one.action_all_update_files" />

		<act_window name="Files to Process" 
			res_model="lx.update.file" 
			id="lx-one.action_update_files_to_process"
			domain="[('state', '!=', 'done')]" />

		<act_window name="All Updates" 
			res_model="lx.update.node" 
			id="lx-one.action_all_update_nodes" />

		<act_window name="Updates to Execute" 
			res_model="lx.update.node" 
			id="lx-one.action_update_nodes_to_execute"
			domain="[('state', '!=', 'executed')]" />

		<act_window name="Configure LX1 Connection" 
			res_model="ir.config_parameter" 
			id="lx-one.action_config" 
			domain="[('key', 'like', 'lx_%')]" />
			
	</data>
</openerp>
